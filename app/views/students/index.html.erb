<h1>Students</h1>

<% @students.each do |student| %>
    <ul>
        <li><%= link_to student.last_name, student_path(student) %>
           <%= link_to 'Edit', edit_student_path(student) %>
           <%= link_to 'Delete', student, method: :delete %> </li>
    </ul>
<%end%>
<br>
<%= link_to "Add Student", new_student_path %>